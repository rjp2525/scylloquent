{
  "name": "danielhe4rt/scylloquent",
  "description": "Cassandra/ScyllaDB driver for Eloquent ORM",
  "keywords": [
    "laravel",
    "lumen",
    "eloquent",
    "cassandra",
    "csql",
    "database",
    "model",
    "scylladb"
  ],
  "homepage": "https://github.com/danielhe4rt/scylloquent",
  "authors": [
    {
      "name": "Ahmedul Haque Abid",
      "role": "Developer",
      "homepage": "https://github.com/a-h-abid",
      "email": "a_h_abid@hotmail.com"
    },
    {
      "name": "Daniel Reis",
      "role": "Developer",
      "homepage": "https://github.com/danielhe4rt",
      "email": "danielhe4rt@gmail.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.2",
    "ext-cassandra": "^1.3",
    "illuminate/support": "^9.0 | ^10.0 | ^11.0",
    "illuminate/container": "^9.0 | ^10.0 | ^11.0",
    "illuminate/database": "^9.0 | ^10.0 | ^11.0",
    "illuminate/console": "^9.0 | ^10.0 | ^11.0",
    "illuminate/events": "^9.0 | ^10.0 | ^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0 | ^9.0 | ^10.0",
    "orchestra/testbench": "^8.1",
    "mockery/mockery": "^1.2",
    "nunomaduro/collision": "^7.4",
    "nunomaduro/larastan": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "DanielHe4rt\\Scylloquent\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DanielHe4rt\\Scylloquent\\Tests\\": "tests/",
      "DanielHe4rt\\Scylloquent\\Fixtures\\": "tests/fixtures/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
